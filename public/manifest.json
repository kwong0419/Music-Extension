{
  "manifest_version": 2,

  "name": "Music Extension",
  "description": "Add Music From Youtube to Spotify",
  "version": "0.0.1",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Add to Spotify"
  },
  "icons": {
    "16": "logo-small.png",
    "48": "logo-small.png",
    "128": "logo-small.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "*://*.youtube.com/*",
    "youtube.com/",
    "storage"
  ]
}
